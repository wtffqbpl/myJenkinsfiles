pipeline {
    agent any

    stages {
        stage("Hello") {
            steps {
                echo "Hello world"
            }
        }

        stage("Build") {
            steps {
                echo "Building project."
            }
        }

        stage("Test") {
            steps {
                echo "Testing project."
            }
        }

        stage("Deploy") {
            steps {
                echo "Deploying project."
            }
        }

        stage("Finish") {
            steps {
                echo "Finishing."
            }
        }
    }
}
